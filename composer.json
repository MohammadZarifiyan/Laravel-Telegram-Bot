{
    "name": "mohammad-zarifiyan/telegram",
    "description": "A package that helps to create Telegram bots easily.",
    "type": "library",
    "keywords": ["Telegram", "Laravel"],
    "version": "2.9",
    "license": "MIT",
    "require": {
        "php": "^8",
        "illuminate/routing": ">=v7",
        "illuminate/validation": ">=7",
        "illuminate/support": ">=7",
        "illuminate/http": ">=7"
    },
    "autoload": {
        "psr-4": {
            "MohammadZarifiyan\\Telegram\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Mohammad Zarifiyan",
            "email": "mohammadzarifiyan82@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "MohammadZarifiyan\\Telegram\\Providers\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "MohammadZarifiyan\\Telegram\\Facades\\Telegram"
            }
        }
    }
}
