    "keywords": ["Telegram", "Laravel", "Lumen"],
    "require": {
        "php": "^8",
        "illuminate/http": "^7|^8|^9",
        "illuminate/container": "^7|^8|^9"
    },
    "autoload": {
        "psr-4": {
            "MohammadZarifiyan\\Telegram\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mohammad Zarifiyan",
            "email": "mohammadzarifiyan82@gmail.com"
        }
    ],
    "extra": {
